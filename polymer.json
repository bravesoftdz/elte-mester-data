{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-browse.html",
    "src/my-search.html",
    "src/my-files.html",
    "src/my-settings.html",
    "src/my-about.html",
    "src/my-view404.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json",
    "bower_components/pdfjs-dist/build/pdf.worker.min.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "autoBasePath": true,
  "builds": [
    {
      "preset": "es6-unbundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es5-bundled"
    },
    {
      "name": "debug"
    }
  ]
}
